<UserControl x:Class="ZuneSocialTagger.GUIV2.WebsiteAlbumMetaDataViewMock"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Grid x:Name="LayoutRoot" Height="84">
        <StackPanel Orientation="Horizontal">
		<Border BorderBrush="White" BorderThickness="1" >
			<Image Source="Assets/blankartwork.png" Stretch="Fill" Height="84" Width="84"/>
		</Border>
           
            <Border VerticalAlignment="Top">
                <StackPanel x:Name="AlbumDetails" Margin="10,0,0,0">
                    <TextBlock x:Name="Title" Text="The Resistance" FontFamily="Assets/Fonts/#Zegoe UI - U" FontSize="13.333" />
                    <TextBlock x:Name="Artist" Text="Muse" Foreground="#FF8A8D90" FontFamily="/ZuneSocialTagger.GUI;Component/Assets/Fonts/#Zegoe UI - U" />
                    <TextBlock x:Name="Song_Count" Text="11 songs" Foreground="#FF8A8D90" FontFamily="/ZuneSocialTagger.GUI;Component/Assets/Fonts/#Zegoe UI - U"  />
                    <TextBlock x:Name="Year" Text="2009" Foreground="#FF8A8D90" FontFamily="/ZuneSocialTagger.GUI;Component/Assets/Fonts/#Zegoe UI - U"  />
                </StackPanel>
            </Border>

        </StackPanel>
    </Grid>


</UserControl>
